import java.util.*;
class Main{
    int lengthOfString(String str)
    {
        if(str.equals(""))
        return 0;
        else
        return lengthOfString(str.substring(1))+1;
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        String str=sc.next();
        System.out.print(obj.lengthOfString(str));
    }
}
