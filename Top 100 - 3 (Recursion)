import java.util.*;
class Main{
    void Reverse(int n)
    {
     if(n<10)
     {
     System.out.print(n);
     return;
     }
     System.out.print(n%10);
     Reverse(n/10);
     }
    
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        obj.Reverse(n);
    }
}
