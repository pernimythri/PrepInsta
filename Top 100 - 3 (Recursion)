import java.util.*;
class Main{
    int powerOfNum(int n,int pow)
    {
        if(pow==0)
        return 1;
        else
        return n*powerOfNum(n,pow-1);
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int pow=sc.nextInt();
        System.out.print(obj.powerOfNum(n,pow));
    }   
}
