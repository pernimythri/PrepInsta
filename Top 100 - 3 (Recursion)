import java.util.*;
class Main{
    String Prime(int n,int i)
    {
       if(n<=2)
       return n==2 ? "Prime" : "Not Prime" ;
       if(n%i==0)
       return "Not Prime";
       if(i*i>n)
       return "Prime";
       else
       return Prime(n,i+1);  
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.print(obj.Prime(n,2));
    }   
}
