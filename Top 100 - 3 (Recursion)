import java.util.*;
class Main{
    int HCF(int n1,int n2)
    {
        
        if(n2==0)
        return n1;
        else
        return HCF(n2,n1%n2);
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.print(obj.HCF(n1,n2));
    }
}
-----------------------------------------------------------------------------------------
import java.util.*;
class Main{
    int HCF(int n1,int n2)
    {
        
        if(n2==0)
        return n1;
        else
        return HCF(n2,n1%n2);
    }
        int lcm(int n1, int n2) 
        {
        return n1 * (n2/HCF(n1, n2));
        }
    
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.print(obj.lcm(n1,n2));
    }
}
