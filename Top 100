import java.util.*;
class Main{
    int SumofNaturals(int n)
    {
        int sum=0;                                       or          return n*(n+1)/2;
        for(int i=1;i<=n;i++)
        sum+=i;
        return sum;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        Main obj=new Main();
        int n=sc.nextInt();
        System.out.println(obj.SumofNaturals(n));
    }
}

