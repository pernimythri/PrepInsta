import java.util.*;
class Main{
    String EvenOdd(int n)
    {
       return n%2==0 ? "Even" : "Odd";          or        return (n&1)==0 ? "Even" : "Odd" ;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        Main obj=new Main();
        int n=sc.nextInt();
        System.out.println(obj.EvenOdd(n));
    }
}

