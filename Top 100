import java.util.*;
class Main{
    String PosNeg(int n)
    {
        if(n==0)
        return "Zero";
        else if(n>0)
        return "Pos";
        else
        return  "Neg";
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        Main obj=new Main();
        int n=sc.nextInt();
        System.out.println(obj.PosNeg(n));
    }
}
