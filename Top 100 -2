import java.util.*;
class Main{
       int octToDec(int n)
       {
           int sol=0,i=0;
           while(n>0)
           {
               int rem=n%10;
               sol+=rem*Math.pow(8,i);
               i++;
               n/=10;
           }
          return sol;
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.print(obj.octToDec(n));
    }
}
