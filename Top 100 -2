class Main{
      int hexToDec(String n)
      {
          int sol=Integer.parseInt(n,16);
          return sol;
      }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        String n=sc.next();
        System.out.print(obj.hexToDec(n));
    }
}
