import java.util.*;
class Main{
       int Gcd(int n1, int n2) {
        if (n2 == 0) {
            return n1;
        }
        return Gcd(n2, n1 % n2);
       
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.print(obj.Gcd(n1,n2));
    }
}
