import java.util.*;
class Main{
       int lcm(int n1, int n2) {
           int gcd=1;
           for(int i=1;i<=Math.min(n1,n2);i++)
           {
               if(n1%i==0 && n2%i==0)
               {
                   gcd=i;
               }
           }
           return (n1*n2)/gcd;
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.print(obj.lcm(n1,n2));
    }
}
