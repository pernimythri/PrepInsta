import java.util.*;
class Main{
       int lcm(int n1, int n2) {
           int max=Math.max(n1,n2);
           int sol=1;
           for(int i=max;i<=n1*n2;i++)
           {
               if(i%n1==0 && i%n2==0)
               {
                   sol=i;
               }
           }
           return sol;
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.print(obj.lcm(n1,n2));
    }
}


