import java.util.*;
class Main{
       int binToDec(int n)
       {
           int sol=0,i=0;
           while(n>0)
           {
               int rem=n%10;
               sol+=rem*Math.pow(2,i);
               i++;
               n/=10;
           }
          return sol;
    }
    public static void main(String args[])
    {
        Main obj=new Main();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.print(obj.binToDec(n));
    }
}

